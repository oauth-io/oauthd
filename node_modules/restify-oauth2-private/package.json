    "coffee-script": ">= 1.6.2",
    "chai": ">= 1.6.0",
    "jshint": ">= 2.1.2",
    "mocha": ">= 1.9.0",
    "restify": ">= 2.5.1",
    "sinon": ">= 1.7.2",
